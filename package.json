{
  "name": "tuscom_web_ridaisai_2016",
  "version": "0.1.0",
  "description": "TUSCOMの2016年理大祭用ページ作成リポジトリ",
  "scripts": {
    "start": "npm-run-all build server",
    "server": "node ./server.js",
    "build": "npm-run-all -s clean -p copy:* -p build:*",
    "build:stylus": "stylus -c src/stylus/style.styl -o dist",
    "build:image": "imagemin src/img/* --out-dir=dist/img",
    "build:webpack": "webpack --progress",
    "copy:html": "cpx ./src/*.html ./dist",
    "copy:slick-carousel": "cpx ./node_modules/slick-carousel/slick/*.css ./dist/slick",
    "clean": "rimraf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/6314089/tuscom_web_ridaisai_2016"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-env": "^0.0.7",
    "cpx": "^1.5.0",
    "eslint": "^3.10.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "imagemin-cli": "^3.0.0",
    "npm-run-all": "^3.1.1",
    "rimraf": "^2.5.4",
    "stylus": "^0.54.5",
    "webpack": "2.1.0-beta.25"
  },
  "dependencies": {
    "jquery": "^3.1.1",
    "jquery-migrate": "^3.0.0",
    "slick-carousel": "^1.6.0"
  }
}
